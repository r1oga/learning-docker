{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "r1oga/fib-calc-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "api",
      "image": "r1oga/fib-calc-api",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "worker",
      "image": "r1oga/fib-calc-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "r1oga/fib-calc-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [{ "hostPort": 80, "containerPort": 80 }],
      "links": ["api", "client"],
      "memory": 128
    }
  ]
}
